    "reset2": "npx prisma migrate reset --schema ./prisma-features-database/schema.prisma",
    "deploy": "npx prisma migrate deploy --schema ./prisma-user-database/schema.prisma && npx prisma migrate deploy --schema ./prisma-features-database/schema.prisma",
    "studio": "concurrently \"bun x prisma studio --schema ./prisma-user-database/schema.prisma --port 5555\" \"bun x prisma studio --schema ./prisma-features-database/schema.prisma --port 5556\""
